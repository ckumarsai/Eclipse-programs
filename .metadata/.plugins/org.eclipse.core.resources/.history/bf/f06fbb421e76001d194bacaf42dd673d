
public class primenumberArray {

	public static void main(String[] args) {
		int primearr[]= {2,4,3,5,7,11};
		for(int i=0;i<primearr.length;i++) {
			int count=0;
			for(int j=2;j<primearr[i];j++) {
				if(primearr[i]%j==0) {
					count++;
				}
			}
			if(count==0) {
				System.out.println(primearr[i] + " is prime");
			}
		}
	}

}
