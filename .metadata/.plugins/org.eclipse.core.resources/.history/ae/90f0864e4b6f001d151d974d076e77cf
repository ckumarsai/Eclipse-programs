package com.innominds.practice;

public class SearchInsertPosition {

	public static void main(String[] args) {
		int arr[]= {1,3,5,6};
		int n=arr.length;
		System.out.println("SearchInsertPosition is:" +findIndex(arr,n,k));

	}
	public static int findIndex(int arr[],int n,int k) {
		for(int i=0;i<n;i++) {
			if(arr[i]==k)
				return i;
			else if(arr[i]>k) {
				return i;
				
			}
		}
		return n;
	}
	

}
